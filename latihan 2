list=[]
x = str(input("Sebiru hari ini: "))
x = x.split(',')
list = [int(i) for i in x]
print("list awal insertion sort:",list)
for i in range(1,len(list)):
    temp = list[i]
    while i>0 and list[i-1]>temp:
        list[i] = list[i-1]
        i =i-1
        list[i] = temp
print("list sesudah insertion sort:",list)
